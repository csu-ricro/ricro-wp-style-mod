  "main": "index.js",
  "scripts": {
    "start": "node-sass --watch src/index.scss dist/ricro-wp-style-mod.css",
    "build": "node-sass src/index.scss dist/ricro-wp-style-mod.css && postcss dist/ricro-wp-style-mod.css > dist/ricro-wp-style-mod.min.css",
    "start:bootstrap4": "node-sass --watch bootstrap4/src/index.scss bootstrap4/dist/colostate-ricro-bootstrap4.css",
    "build:bootstrap4": "node-sass bootstrap4/src/index.scss bootstrap4/dist/colostate-ricro-bootstrap4.css && postcss bootstrap4/dist/colostate-ricro-bootstrap4.css > bootstrap4/dist/colostate-ricro-bootstrap4.min.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
